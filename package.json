{
  "name": "bzz-wip",
  "version": "0.0.1-alpha.1",
  "description": "Pruebas de concepto",
  "scripts": {
    "lint": "node node_modules/.bin/tslint src/**/*.ts --fix",
    "sass": "node node_modules/.bin/sass-lint -c .scss-lint.yml 'src/**/*.scss' -v -q --fix",
    "stylelint": "node node_modules/.bin/stylelint 'src/**/*.scss' --config stylelint.json --syntax sass --fix",
    "start": "node build/dev-server.js",
    "start:prod": "NODE_ENV=production node build/dev-server.js",
    "build": "node build/build-prod.js",
    "server": "node node node_modules/.bin/http-server dist --cors",
    "build:dev": "npm run clean && webpack --config build/webpack.dev.conf.js --progress --profile",
    "clean": "node node_modules/.bin/rimraf dist",
    "debug": "node --nolazy --inspect=9229 build/build-prod.js",
    "test": "node node_modules/.bin/karma start test/karma.conf.js",
    "e2e": "node node_modules/.bin/protractor e2e/protractor.conf.js",
    "pree2e": "node node_modules/typescript/bin/tsc --project e2e/tsconfig.e2e.json && node node_modules/protractor/bin/webdriver-manager update"
  },
  "keywords": [
    "pruebas",
    "conceptos"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krbaio3/bzz.git"
